{
  "name": "Astro Blogger",
  "description": "An Astro theme for blogging that demonstrates concepts such as index page pagination and RSS feed with full post content.",
  "version": "0.0.3",
  "scripts": {
    "dev": "export NODE_ENV=development && astro dev",
    "dev:h": "export NODE_ENV=development && astro dev --host",
    "start": "npm run dev",
    "start:h": "npm run dev:h",
    "build": "export NODE_ENV=production && astro build",
    "preview": "astro preview"
  },
  "keywords": [
    "astro",
    "blog"
  ],
  "dependencies": {
    "@astrojs/image": "^0.7.1",
    "@astrojs/mdx": "^0.11.1",
    "@astrojs/rss": "^1.0.1",
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/solid-js": "^1.1.0",
    "astro": "^1.3.0",
    "astro-embed": "^0.1.1",
    "astro-icon": "^0.7.3",
    "config": "^3.3.8",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "normalize.css": "^8.0.1",
    "remark-gfm": "^3.0.1",
    "solid-js": "^1.5.6"
  },
  "devDependencies": {
    "astro-eslint-parser": "^0.6.1",
    "eslint": "^8.24.0",
    "eslint-plugin-astro": "^0.19.0",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.4"
  }
}
